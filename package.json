  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diracleo/vue-scrubbable-video/issues"
  },
  "homepage": "https://github.com/diracleo/vue-scrubbable-video#readme",
  "keywords": [
    "vue",
    "vue-scrubbable-video",
    "video scrubbing",
    "video scrubber",
    "scrubber",
    "video scrolling",
    "video scroller",
    "video frames",
    "scrubbing"
  ]
